@{
  Layout = "_Layout";
}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
@model HairSalon.Models.Stylist

<h2>Are you sure you want to delete this Stylist?</h2>

<p>@Html.DisplayNameFor(model => model.Name): @Html.DisplayFor(model=>model.Name)</p>
@using (Html.BeginForm())
{
    <input type="submit" value="Delete" class="btn btn-info btn-sm"/>
}
<br>
@Html.ActionLink("Back to Stylists", "Index")
